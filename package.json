    "@actions/tool-cache": "1.7.2"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.22",
    "@typescript-eslint/parser": "5.16.0",
    "@vercel/ncc": "0.33.3",
    "eslint": "8.11.0",
    "eslint-plugin-github": "4.3.5",
    "eslint-plugin-jest": "^26.1.2",
    "jest": "26.6.3",
    "jest-circus": "27.5.1",
    "js-yaml": "4.1.0",
    "prettier": "2.6.0",
    "ts-jest": "26.5.6",
    "typescript": "4.6.2"
  }
