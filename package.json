    "@actions/tool-cache": "2.0.1"
  },
  "devDependencies": {
    "@types/jest": "27.5.1",
    "@types/node": "17.0.36",
    "@typescript-eslint/parser": "5.27.0",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.16.0",
    "eslint-plugin-github": "4.3.6",
    "eslint-plugin-jest": "^26.2.2",
    "jest": "28.1.0",
    "jest-circus": "28.1.0",
    "js-yaml": "4.1.0",
    "prettier": "2.6.2",
    "ts-jest": "28.0.3",
    "typescript": "4.7.2"
  }
