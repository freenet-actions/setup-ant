    "node",
    "setup"
  ],
  "author": "mtbui01",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/tool-cache": "1.6.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/node": "15.12.4",
    "@typescript-eslint/parser": "4.27.0",
    "@vercel/ncc": "0.27.0",
    "eslint": "7.19.0",
    "eslint-plugin-github": "4.1.2",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "js-yaml": "4.0.0",
    "prettier": "2.2.1",
    "ts-jest": "26.5.0",
    "typescript": "4.1.3"
  }
