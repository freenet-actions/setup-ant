{
  "name": "setup-ant",
  "version": "1.0.3",
  "private": true,
  "description": "setup ant",
  "main": "lib/setup-ant.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/md-actions/setup-ant.git"
  },
