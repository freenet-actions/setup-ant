  "dependencies": {
    "@actions/core": "1.4.0",
    "@actions/tool-cache": "1.7.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/node": "16.3.0",
    "@typescript-eslint/parser": "4.28.3",
    "@vercel/ncc": "0.29.0",
    "eslint": "7.30.0",
    "eslint-plugin-github": "4.1.5",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "26.6.3",
    "jest-circus": "27.0.6",
    "js-yaml": "4.1.0",
    "prettier": "2.3.2",
    "ts-jest": "26.5.6",
    "typescript": "4.3.5"
