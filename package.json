{
  "name": "setup-ant",
  "version": "2.0.0",
  "private": true,
  "description": "setup ant",
  "main": "lib/setup-ant.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
